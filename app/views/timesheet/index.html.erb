<div class="row">
<div class="col-md-4 col-md-offset-4 text-center">
<div class="panel panel-default panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Hours</h3>
  </div>
  <div class="panel-body">
    <p><%= Date.today.strftime("%A, %B %d, %Y") %></p>

    <div class="container-fluid">
      <h2><%= current_user.name.capitalize %></h2>

      <form action="#">
 
        <fieldset>
          <label for="job">Select Job</label>
          <select name="speed" id="job">
            <option selected="selected">Job 1</option>
            <option>Job 2</option>
            <option>Job 3</option>
            <option>Job 4</option>
            <option>Job 5</option>
          </select>
        </fieldset>
      </form>
      <br>

      <input type="text" value="8" class="dial" data-linecap=round data-fgColor="#66CC66" data-bgColor="#eee">
      
      <div>
        <button type="button" class="btn btn-primary">Submit</button> 
      </div>
    </div>
  </div>
</div>
</div>
</div>


  <style>
    fieldset {
      border: 0;
    }
    label {
      display: block;
      margin: 30px 0 0 0;
    }
    select {
      width: 200px;
    }
    .overflow {
      height: 200px;
    }
  </style>

<script>

  $(".dial").knob({
    'min':0,
    'max':24
  });
</script>

<script>
  $(function() {
    $( "#job" ).selectmenu();
  });
  </script>